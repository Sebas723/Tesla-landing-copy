---
import HeroSection from "../components/IndexComponents/HeroSection.astro";
import LandingHeader from "../components/LandingHeader.astro"
import Layout from "../layouts/Layout.astro";
import SectionModelY from "../components/IndexComponents/SectionModelY.astro";
import SectionModelX from "../components/IndexComponents/SectionModelX.astro";
import SectionModelS from "../components/IndexComponents/SectionModelS.astro";
import SectionCyberTruck from "../components/IndexComponents/SectionCyberTruck.astro";
import SectionSolarPanels from "../components/IndexComponents/SectionSolarPanels.astro";
import SectionSolarRoof from "../components/IndexComponents/SectionSolarRoof.astro";
import SectionPowerall from "../components/IndexComponents/SectionPowerall.astro";
import SectionAccessories from "../components/IndexComponents/SectionAccessories.astro";
---

<Layout title="Welcome to Astro.">
  <LandingHeader />
  <main class="snap-y snap-mandatory relative w-full h-screen overflow-auto">
    <div class="snap-center">
      <HeroSection />
    </div>
    <div class="snap-center">
      <SectionModelY />
    </div>
    <div class="snap-center">
      <SectionModelX />
    </div>
    <div class="snap-center">
      <SectionModelS />
    </div>
    <div class="snap-center">
      <SectionCyberTruck />
    </div>
    <div class="snap-center">
      <SectionSolarPanels />
    </div>
    <div class="snap-center">
      <SectionSolarRoof />
    </div>
    <div class="snap-center">
      <SectionPowerall />
    </div>
    <div class="snap-center">
      <SectionAccessories />
    </div>

  </main>
</Layout>
